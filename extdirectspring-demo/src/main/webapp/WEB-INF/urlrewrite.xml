<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.2//EN" "http://www.tuckey.org/res/dtds/urlrewrite3.2.dtd">

<urlrewrite>

	<rule>
		<from>^/simple$</from>
		<to  type="permanent-redirect">/demo/extjs4/simple/index.html</to>
	</rule>
	<rule>
		<from>^/direct-named-arguments.html$</from>
		<to type="permanent-redirect">/demo/extjs4/named-arguments.html</to>
	</rule>
	<rule>
		<from>^/direct4.html$</from>
		<to type="permanent-redirect">/demo/extjs4/direct.html</to>
	</rule>
	<rule>
		<from>^/direct-(.*)4\.html$</from>
		<to type="permanent-redirect">/demo/extjs4/$1.html</to>
	</rule>
	

	<rule>
		<from>^/direct.html$</from>
		<to type="permanent-redirect">/demo/extjs3/direct.html</to>
	</rule>
	<rule>
		<from>^/direct-(.*)\.html$</from>
		<to type="permanent-redirect">/demo/extjs3/$1.html</to>
	</rule>	
	
	<rule>
		<from>^/touchexample$</from>
		<to type="permanent-redirect">/demo/touch2/index.html</to>
	</rule>		
	

</urlrewrite>